<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:based="clr-namespace:WpfExplorer.Support.UI.Units;assembly=WpfExplorer.Support"
    xmlns:views="clr-namespace:WpfExplorer.Forms.UI.Views">

    <Style TargetType="{x:Type views:ExplorerWindow}" 
           BasedOn="{StaticResource {x:Type based:DarkWindow}}">

        <Setter Property="Tag">
            <Setter.Value>
                <StackPanel Orientation="Horizontal">
                    <Button Content="11"/>
                    <Button Content="22"/>
                    <Button Content="33"/>
                </StackPanel>
            </Setter.Value>
        </Setter>
        
        <Setter Property="Content">
            <Setter.Value>
                <StackPanel>
                    <Slider/>
                    <TextBox/>
                    <CheckBox/>
                    <ComboBox/>
                </StackPanel>
            </Setter.Value>
        </Setter>

       
        
    </Style>
</ResourceDictionary>
